<launch>
	<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
		<param name="port" value="/dev/ttyACM0"/>
	</node>
		<node pkg="me212_robot" type="wheel_odometry.py" name="wheel_odometry">
	</node>
	<!--node pkg="me212_robot" type="tracking.py" name="tracking"-->
	<!--/node-->

	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find me212_robot)/config/odometry.rviz" />
    <param name="robot_description" command="$(find xacro)/xacro.py $(find me212_robot)/model/me212_robot.xacro"/>
</launch>
